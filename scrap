javascript:(function(){let category=prompt('Enter a category:');let data='#'+category+'\n\n';let elements=document.getElementsByTagName('p');for(let i=0;i<elements.length;i++){data+=elements[i].innerText+'\n\n';}let blob=new Blob([data],{type:'text/markdown'});let url=URL.createObjectURL(blob);let a=document.createElement('a');a.href=url;a.download='data.md';a.click();})();
